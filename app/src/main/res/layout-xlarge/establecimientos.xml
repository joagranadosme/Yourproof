<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/Cont_Principal">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:id="@+id/Cont_Encabezado" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:baselineAligned="false" >

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.1" >

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="YOURPROOF"
                    android:id="@+id/textView"
                    android:textSize="@dimen/abc_text_size_large_material"
                    android:gravity="left|center"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/background_floating_material_light" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.9" >

                <ImageView
                    android:src="@drawable/g"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/imageView13"
                    android:scaleType="centerCrop" />
            </LinearLayout>
        </LinearLayout>
    </TableLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:baselineAligned="false"
        android:layout_gravity="center_horizontal"
        android:background="@color/accent_material_dark"
        android:id="@+id/Cont_Banner">

        <ImageView
            android:src="@drawable/barrasuperiorestablecimientos"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/VEstablecimientos"
            android:scaleType="fitCenter"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="ESTABLECIMIENTOS"
            android:id="@+id/VProductos"
            android:textSize="@dimen/abc_text_size_large_material"
            android:textColor="@color/Letra"
            android:textAlignment="center"
            android:layout_weight="5"
            android:layout_marginTop="1dp"
            android:background="@color/colorAccent" />

    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:id="@+id/Cont_Iconos">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <ImageButton
                android:layout_weight="1"
                android:src="@drawable/e_btn_hoteles"
                android:id="@+id/BtnHoteles"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:layout_width="220dp"
                android:layout_height="220dp"
                android:contextClickable="false"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="false" />

            <ImageButton
                android:layout_weight="1"
                android:src="@drawable/e_btn_restaurantes"
                android:layout_width="220dp"
                android:layout_height="220dp"
                android:id="@+id/BtnRestaurantes"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="false"
                android:layout_alignParentStart="true" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="HOTELES"
                android:id="@+id/VHoteles"
                android:textAlignment="center"
                android:textColor="@color/Letra"
                android:layout_marginBottom="10dp"
                android:textSize="@dimen/abc_text_size_large_material" />

            <TextView
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="RESTAURANTES"
                android:id="@+id/VRestaurantes"
                android:textColor="@color/Letra"
                android:textAlignment="center"
                android:layout_marginBottom="10dp"
                android:textSize="@dimen/abc_text_size_large_material" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <ImageButton
                android:layout_weight="1"
                android:src="@drawable/e_btn_bares"
                android:id="@+id/BtnBares"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:layout_width="220dp"
                android:layout_height="220dp"
                android:contextClickable="false"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="false" />

            <ImageButton
                android:layout_weight="1"
                android:src="@drawable/e_btn_almacenes"
                android:layout_width="220dp"
                android:layout_height="220dp"
                android:id="@+id/BtnAlmacenes"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="false"
                android:layout_alignParentStart="true" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="BARES"
                android:id="@+id/VBares"
                android:textAlignment="center"
                android:textColor="@color/Letra"
                android:layout_marginBottom="10dp"
                android:textSize="@dimen/abc_text_size_large_material" />

            <TextView
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="ALMACENES DE CADENA"
                android:id="@+id/VAlmacenesC"
                android:textColor="@color/Letra"
                android:textAlignment="center"
                android:layout_marginBottom="10dp"
                android:textSize="@dimen/abc_text_size_large_material" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageButton
                android:layout_weight="1"
                android:src="@drawable/e_btn_clubes"
                android:layout_width="220dp"
                android:layout_height="220dp"
                android:id="@+id/BtnClubes"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:minHeight="200dp"
                android:layout_column="1"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <ImageButton
                android:src="@drawable/e_btn_panaderias"
                android:layout_width="220dp"
                android:layout_height="220dp"
                android:id="@+id/BtnPanaderias"
                android:scaleType="fitCenter"
                android:layout_weight="1"
                android:background="@color/transparent"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="CLUBES"
                android:id="@+id/VCubles"
                android:textAlignment="center"
                android:textColor="@color/Letra"
                android:textSize="@dimen/abc_text_size_large_material" />

            <TextView
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="PANADERIAS"
                android:id="@+id/VPanaderias"
                android:textColor="@color/Letra"
                android:textAlignment="center"
                android:textSize="@dimen/abc_text_size_large_material" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center" >

            <ImageView
                android:layout_weight="1"
                android:src="@drawable/recetasblanco"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:id="@+id/Vinvisible3"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <ImageButton
                android:src="@drawable/e_btn_otros"
                android:layout_width="170dp"
                android:layout_height="170dp"
                android:id="@+id/BtnOtrosE"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_weight="1" />

            <ImageView
                android:layout_weight="1"
                android:src="@drawable/recetasblanco"
                android:layout_width="85dp"
                android:layout_height="85dp"
                android:id="@+id/Vinvisible4"
                android:scaleType="fitCenter"
                android:background="@color/transparent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
        </LinearLayout>

    </TableLayout>

</LinearLayout>